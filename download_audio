for channel in $(<channels.lst); do
  PYTHONPATH="" /home/john/yt/.venv/bin/yt-dlp -f 140 --restrict-filenames -o "audio/%(channel)s/%(upload_date>%Y-%m-%d)s_%(title)s_%(id)s.%(ext)s" --sleep-requests 1 --sleep-interval 3 --remote-components ejs:github --break-on-existing --download-archive .archive "https://www.youtube.com/$channel/videos"
  PYTHONPATH="" /home/john/yt/.venv/bin/yt-dlp -f 140 --restrict-filenames -o "audio/%(channel)s/live/%(upload_date>%Y-%m-%d)s_%(title)s_%(id)s.%(ext)s" --sleep-requests 1 --sleep-interval 3 --remote-components ejs:github --break-on-existing --download-archive .archive "https://www.youtube.com/$channel/streams"
  PYTHONPATH="" /home/john/yt/.venv/bin/yt-dlp -f 140 --restrict-filenames -o "audio/%(channel)s/shorts/%(upload_date>%Y-%m-%d)s_%(title)s_%(id)s.%(ext)s" --sleep-requests 1 --sleep-interval 3 --remote-components ejs:github --break-on-existing --download-archive .archive "https://www.youtube.com/$channel/shorts"
done
